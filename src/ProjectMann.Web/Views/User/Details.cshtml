@model ProjectMann.Core.Domain.Usuario

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Usuario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nombre:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            Apellido:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class = "col-sm-2">
            Nombre de usuario:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreUsuario)
        </dd>
        <dt class = "col-sm-2">
           Correo Electronico:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
           Estado:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>
        <dt class = "col-sm-2">
            Fecha de creación:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCreacion)
        </dd>
        <dt class = "col-sm-2">
           Fecha de modificación:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaModificacion)
        </dd>
        <dt class = "col-sm-2">
            Tipo de usuario:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FkRolNavigation.Nombre)
        </dd>
        <dt class = "col-sm-2">
           Usuario que crea:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FkUsuarioCreaNavigation.Apellido)
        </dd>
        <dt class = "col-sm-2">
            Usuario que modifica:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FkUsuarioModificaNavigation.Apellido)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdUsuario">
        <input type="button" value="Editar" class="btn btn-primary" />
    </a> |
    <a asp-action="Index">
        <input type="button" value="Volver" class="btn btn-secondary" />
    </a>
</div>
